import streamlit as st
import app_01
import app_02

# Title for the app
st.title("LESTARI BAHASA PROTOTIPE")

# Sidebar for navigation
st.sidebar.title("Navigation")
page = st.sidebar.radio("Select a page:", ["Page 1", "Page 2"])

# Load the selected page
if page == "Page 1":
    app_01.main()
elif page == "Page 2":
    app_02.main()
